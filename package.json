{
  "name": "SkyMusic",
  "productName": "Sky Music",
  "version": "1.0.0",
  "description": "Create sky music sheets here.",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Medic#0756",
    "email": "HNmedic@outlook.com"
